# result_out CMake config file
#
# This file sets the following variables:
# result_out_FOUND - Always TRUE.
# result_out_INCLUDE_DIRS - Directories containing the result_out include files.
# result_out_IDL_DIRS - Directories containing the result_out IDL files.
# result_out_LIBRARIES - Libraries needed to use result_out.
# result_out_DEFINITIONS - Compiler flags for result_out.
# result_out_VERSION - The version of result_out found.
# result_out_VERSION_MAJOR - The major version of result_out found.
# result_out_VERSION_MINOR - The minor version of result_out found.
# result_out_VERSION_REVISION - The revision version of result_out found.
# result_out_VERSION_CANDIDATE - The candidate version of result_out found.

message(STATUS "Found result_out-@PROJECT_VERSION@")
set(result_out_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(result_out_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(result_out_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(result_out_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(result_out_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(result_out_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(result_out_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(result_out_DEFINITIONS ${<dependency>_DEFINITIONS})

set(result_out_VERSION @PROJECT_VERSION@)
set(result_out_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(result_out_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(result_out_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(result_out_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

